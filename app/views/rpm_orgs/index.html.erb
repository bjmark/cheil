<h1>Listing RPM</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Cheil</th>
    <th>Users</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @rpm_orgs.each do |r| %>
    <tr>
      <td><%= r.name %></td>
      <td><%= r.cheil_org.name %></td>
      <td> <% r.users.each  do |e| %>
          <%= link_to e.name,user_path(e) %>
        <%end%> </td>
      <td><%= link_to 'Show', r %></td>
      <td><%= link_to 'Edit', edit_rpm_org_path(r) %></td>
      <td><%= link_to 'Destroy', r, confirm: 'Are you sure?', method: :delete %></td>
      <td><%= link_to 'Create user', new_user_path(:org_id=>r.id) %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New RPM', new_rpm_org_path %>
