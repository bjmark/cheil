<%= form_tag(@path,:class=>'form') do -%>
  <div class="group">
    <%@vendors.each do |e|%>
      <div>
        <%= check_box_tag "vendor#{e.id}",e.id,false,:class=>'checkbox'%>
        <%= label_tag "vendor#{e.id}",e.name,:class=>'checkbox' %>
      </div>
    <%end%>
  </div>
  <div class="group">
    <%= submit_tag '保存' %>
  </div>
<% end -%> 
