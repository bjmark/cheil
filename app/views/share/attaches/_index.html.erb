<%
  #share/attaches/_index
#locals=>owner
%>
<table class="table">
  <tbody>
    <tr>
      <th>文件名</th>
      <th></th>
    </tr>
    <%n=1
    owner.attaches.each do |e|%>
      <tr class="<%=['odd','even'][(n+=1)%2]%>">
        <td>
          <%= e.attach_file_name  %>
        </td>
        <td class="last">
          <%=raw attach_links(owner,e,@cur_user) %>
        </td>
      </tr>
    <%end%>
  </tbody>
</table>
<%= new_attach_link(owner,@cur_user) %>
